{
  "name": "sample-iot-core",
  "version": "0.1.0",
  "bin": {
    "sample-iot-core": "bin/sample-iot-core.js"
  },
  "author": {
    "name": "Fawad Mazhar",
    "url": "https://github.com/fawad1985"
  },
  "scripts": {
    "test": "jest",
    "install": "cd functions && npm install",
    "bootstrap:dev": "cdk bootstrap -v --profile fcdev --context stage=dev",
    "synth:dev": "npx cdk -v --profile fcdev synth sample-iot-core-dev",
    "diff:dev": "npx cdk -v --profile fcdev diff sample-iot-core-dev",
    "deploy:dev": "npm run synth:dev && npx cdk -v --profile fcdev deploy sample-iot-core-dev",
    "destroy:dev": "npx cdk -v --profile fcdev destroy sample-iot-core-dev"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.14.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "aws-cdk": "2.155.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.155.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21",
    "esbuild": "^0.19.10"
  }
}